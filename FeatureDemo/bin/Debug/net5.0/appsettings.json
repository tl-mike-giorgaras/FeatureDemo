{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "FeatureManagement": {
    "SimpleFilter": false,
    "PercentageFilter": {
      "EnabledFor": [
        {
          "Name": "Microsoft.Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    },
    "TimeWindowFilter": {
      "EnabledFor": [
        {
          "Name": "Microsoft.TimeWindow",
          "Parameters": {
            "Start": "20 Oct 2021 00:00:00 GMT",
            "End": "21 Oct 2021 00:00:00 GMT"
          }
        }
      ]
    },
    "CustomFilter": {
      "EnabledFor": [
        {
          "Name": "Custom",
          "Parameters": {
            "AllowedAccept": "text/json"
          }
        }
      ]
    }
  }
}
